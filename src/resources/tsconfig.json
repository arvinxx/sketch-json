{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "typings/*": ["../../typings/*"],
      "@/*": ["./*"],
      "@/common": ["../common"],
      "@/common/*": ["../common/*"],
      "@/bridge/*": ["../bridge/*"],
      "@/bridge": ["../bridge"],
      "@@/*": [".umi/*"]
    }
  },
  "include": [
    "mock/**/*",
    "./services/**/*",
    "./pages/**/*",
    "./assets/**/*",
    "config/.umirc.ts",
    "typings.d.ts",
    "../../typings"
  ]
}
